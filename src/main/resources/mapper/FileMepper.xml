<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.fileupload.file.FileMapper">

    <insert id="excelUpload" parameterType="java.util.List">
        INSERT INTO tbl_excelfile
        (excelfile_phone_num, excelfile_name, excelfile_email)
        VALUES
        <foreach collection="list" item="item" separator=",">
            (
                #{item.excelfilePhoneNum},
                #{item.excelfileName},
                #{item.excelfileEmail}
            )
        </foreach>
    </insert>

</mapper>